{
  "name": "english_program",
  "version": "1.1.5",
  "description": "A web application for learning English through interactive lessons and exercises.",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "format": "prettier --write .",
    "lint": "eslint --fix",
    "check-lint": "eslint .",
    "check-format": "prettier --check .",
    "prepare": "husky",
    "last-lint-staged": "lint-staged"
  },
  "lint-staged": {
    "**/*.{js,jsx,tsx,ts,less,scss,md,json}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "keywords": [
    "english",
    "lessons"
  ],
  "author": "AndriiKot",
  "license": "ISC",
  "dependencies": {
    "dotenv": "^16.4.5",
    "express": "^4.21.1",
    "pug": "^3.0.3"
  },
  "devDependencies": {
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "globals": "^15.12.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "nodemon": "^3.1.7",
    "prettier": "^3.3.3"
  }
}
